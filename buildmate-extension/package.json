{
  "name": "buildmate-extension",
  "displayName": "buildmate-extension",
  "description": "An AI accelerated build and dependency system helper plugin. Made during DubHacks 2025",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:buildmate-extension.compileC"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "buildmate-extension.compileC",
        "title": "buildmate: compile"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "form-data": "^4.0.4",
    "node-fetch": "^2.7.0"
  }
}
